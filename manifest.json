{
  "manifest_version": 3,
  "name": "AI Resume Sidekick",
  "version": "1.1",
  "description": "Scans LinkedIn Jobs and Auto-Fills Applications",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["*://*.linkedin.com/*", "http://localhost:3000/*"],
  "action": {
    "default_popup": "popup.html"
  }
}